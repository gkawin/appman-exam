{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "standard",
        "standard-react",
        "plugin:jsx-a11y/recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "rules": {
        "react/sort-comp": [ "error", {
            "order": [
              "constructor",
              "lifecycle",
              "everything-else",
              "/^on.+$/",
              "/^render.+$/",
              "render"
            ]
          } 
        ]        
    }
}